{
  "version": 2,
  "builds": [
    {
      "src": "public/index.php",
      "use": "@vercel/php"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/public/index.php"
    }
  ],
  "env": {
    "APP_ENV": "production",
    "APP_DEBUG": "false",
    "APP_KEY": "@app_key",
    "DB_CONNECTION": "sqlite",
    "CACHE_DRIVER": "file",
    "SESSION_DRIVER": "file",
    "FILESYSTEM_DISK": "public"
  }
}
