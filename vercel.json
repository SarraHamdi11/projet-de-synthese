{
  "version": 2,
  "name": "findstay",
  "builds": [
    {
      "src": "hob/public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/hob/public/index.php"
    }
  ],
  "buildCommand": "cd hob && composer install --no-dev --optimize-autoloader && cd hob && php artisan config:cache && cd hob && php artisan route:cache",
  "outputDirectory": "hob/public",
  "installCommand": "cd hob && composer install --no-dev",
  "framework": null,
  "env": {
    "APP_ENV": "production",
    "APP_DEBUG": "false",
    "APP_KEY": "base64:7ndKZHNfwrc2gNOLPhJEJOFKpo2Og35j2f8ZuweeghA=",
    "DB_CONNECTION": "sqlite",
    "CACHE_DRIVER": "file",
    "SESSION_DRIVER": "file",
    "FILESYSTEM_DISK": "public",
    "APP_URL": "https://findstay.vercel.app"
  }
}
